  "main": "dist/es5/index.js",
  "typings": "dist/es5/index.d.ts",
  "module": "dist/esm/index.js",
  "dependencies": {
    "ionic-native": "^2.2.11",
    "superagent": "1.7.2",
    "@ionic/db": "^0.1.0"
  },
  "devDependencies": {
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "browserify": "^13.1.0",
    "chokidar-cli": "tlancina/chokidar-cli",
    "concurrently": "^2.2.0",
    "cpr": "^2.0.0",
    "dgeni": "^0.4.2",
    "dgeni-packages": "~0.14.0",
    "es6-promise": "^3.2.1",
    "eslint": "^2.10.2",
    "eslint-config-ionic": "*",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-browserify": "^5.0.5",
    "karma-jasmine": "^1.0.2",
